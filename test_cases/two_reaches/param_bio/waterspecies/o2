O2                                               ! Species keyword

oxygen =                                         ! Subspecies name, SPECIES_NUM = 1
{
    ! Advection-diffusion properties
    transport_type = SOLUTE
    media_type     = FREE_WATER
    advection      = yes
    diffusion      = no
    diff_mol       = [m^2/s] 0.
    
    ! Diffusion coefficient at water-sediment interface
    ds  = [m^2/s] 0.0015

    ! Oxygen reaeration at air-water interface
    reaeration = {
        rea_navig = [m/h] 0.
        rea_wind  = [cm/h] 118.
        d_richey  = [cm^2/s] 0.000026
    }
}